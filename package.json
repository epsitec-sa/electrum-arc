    "electrum-theme": "^1.5.0",
    "react": "^0.14.6"
  },
  "dependencies": {
    "electrum": "^2.11.2",
    "electrum-utils": "^1.1.0",
    "leaflet": "^0.7.7",
    "velocity-animate": "^1.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-transform-decorators": "^6.3.13",
