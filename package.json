  },
  "bugs": {
    "url": "https://github.com/epsitec-sa/electrum-arc/issues"
  },
  "homepage": "https://github.com/epsitec-sa/electrum-arc",
  "peerDependencies": {
    "electrum": ">=1.2",
    "react": "^0.14.3"
  },
  "dependencies": {
    "electrum-utils": "1.x",
    "leaflet": "0.7.x",
    "velocity-animate": "1.2.x"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-plugin-transform-decorators": "^6.1.18",
